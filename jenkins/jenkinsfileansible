pipeline {
    agent { label 'ubuntu_agent' }
    stages {
        stage('Ansible-Playbook Execution') {
            steps {
                echo 'Executing Ansible Playbook'
                ansiblePlaybook credentialsId: 'foransible', inventory: 'ansible/hosts', playbook: 'ansible/playbook3.yml', vaultTmpPath: ''
            }
        }
    }
}
